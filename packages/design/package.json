    "d3-path": "^3.1.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "eve": "^0.5.4",
    "html2canvas": "^1.4.1",
    "jsbarcode": "^3.11.5",
    "jspdf": "^2.5.1",
    "moveable": "^0.53.0",
    "qrcode": "^1.5.3",
    "selecto": "^1.26.3",
