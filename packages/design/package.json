    "@types/d3-selection": "^3.0.10",
    "@types/d3-shape": "^3.1.6",
    "@types/qrcode": "^1.5.5",
    "@types/snapsvg": "^0.5.8",
    "@vueuse/core": "^10.6.1",
    "element-plus": "^2.4.2",
    "mitt": "^3.0.0",
