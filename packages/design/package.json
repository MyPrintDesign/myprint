{
  "name": "@cp-print/design",
  "version": "0.0.227",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.mjs",
  "types": "dist/es/index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "gulp --require @esbuild-kit/cjs-loader"
  },
  "exports": {
    ".": {
      "require": "./dist/lib/index.js",
      "import": "./dist/es/index.mjs",
      "types": "./dist/es/index.d.ts"
    },
    "./es/*": {
      "import": "./dist/es/*/index.mjs",
      "types": "./dist/es/*/index.d.ts"
    },
    "./lib/*": {
      "require": "./dist/lib/*/index.js",
      "types": "./dist/lib/*/index.d.ts"
    },
    "./css/*": "./dist/css/styles/*"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": "^3.2.37"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "jsbarcode": "^3.11.5",
    "jspdf": "^2.5.1",
    "qrcode": "^1.5.3",
    "vue3-colorpicker": "^2.1.6",
    "vue-cropper": "^1.1.1"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@vueuse/core": "^10.6.1",
    "element-plus": "^2.4.2",
    "vue-demi": "^0.14.6",
    "mitt": "^3.0.0",
    "pinia": "^2.1.4",
    "pinia-plugin-persist": "^1.0.0",
    "sass": "^1.53.0",
    "@types/qrcode": "^1.5.5"
  }
}
